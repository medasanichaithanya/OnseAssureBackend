{
    "builds": [
        {
            "src":"app.py",
            "use":"@vercel/python"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest": "app.py"
        }
    ],
    "build" :{
        "functions":"vercel/functions"
    },
    "functions": {
        "/fetch-premium":{
            "source": "app.py"
        }
    }
}